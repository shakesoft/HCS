    <PackageReference Include="Volo.Abp.Identity.Pro.Application" Version="10.0.2" />
    <PackageReference Include="Volo.Abp.Identity.Pro.HttpApi" Version="10.0.2" />
    <PackageReference Include="Volo.Abp.Identity.Pro.Web" Version="10.0.2" />
