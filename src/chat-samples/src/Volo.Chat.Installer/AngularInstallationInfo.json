{
	"packages":[
		{
			"name": "@volo/abp.ng.chat",
			"appRoutingModuleConfiguration":{
				"routes":[
					"{ path: 'chat', loadChildren: () => import('@volo/abp.ng.chat').then(c => c.createRoutes()), }"
				]
			},
			"appModuleConfiguration":{
				"imports":[
					{
						"names":[
							"provideChatConfig"
						],
						"namespace": "@volo/abp.ng.chat/config"
					}
				],
				"providerNames":[
					"provideChatConfig()"
				]
			},
			"tsJsonPathRecordConfigurations":[
				{
					"name": "@volo/abp.ng.chat",
					"paths": [
						"angular/projects/chat/src/public-api.ts"
					]
				},
				{
					"name": "@volo/abp.ng.chat/config",
					"paths": [
						"angular/projects/chat/config/src/public-api.ts"
					]
				},
				{
					"name": "@volo/abp.ng.chat/proxy",
					"paths": [
						"angular/projects/chat/proxy/src/public-api.ts"
					]
				}
			],
			"replacementPackages":[
				{
					"name": "@microsoft/signalr",
					"version": "7.0.0"
				}
			]
		}
	]
}