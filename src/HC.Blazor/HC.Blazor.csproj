    <PackageReference Include="Volo.Abp.AuditLogging.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.Abp.OpenIddict.Pro.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.Abp.LanguageManagement.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.FileManagement.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.Saas.Host.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.Chat.Blazor.Server" Version="10.0.1" />
    <PackageReference Include="Volo.Chat.SignalR" Version="10.0.1" />
