    <PackageReference Include="Volo.Abp.AspNetCore.Authentication.OpenIdConnect" Version="10.0.1" />
    <PackageReference Include="Volo.Abp.Http.Client.Web" Version="10.0.1" />
    <PackageReference Include="Volo.Abp.Http.Client.IdentityModel.Web" Version="10.0.1" />
