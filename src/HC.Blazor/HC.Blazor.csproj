  <ItemGroup>
    <ProjectReference Include="..\HC.HttpApi.Client\HC.HttpApi.Client.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.AspNetCore.Mvc.UI.Theme.LeptonX" Version="5.0.1" />
